[
  {
    "id": "rule-ru-mobile-casino",
    "enabled": true,
    "match": {
      "path": "^/casino/([^/?#]+)",
      "countries": ["RU"],
      "devices": ["mobile"],
      "bots": false
    },
    "action": {
      "type": "redirect",
      "target": "https://bxxd.ru/tds/go.cgi?6",
      "query": {
        "bonus": { "fromPathGroup": 1 }
      },
      "status": 302,
      "preserveOriginalQuery": false,
      "extraQuery": {
        "src": "mobile-geo"
      },
      "appendCountry": true,
      "appendDevice": true
    }
  }
]
